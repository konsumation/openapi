{
  "name": "@konsumation/openapi",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "openapi/openapi.json"
  ],
  "packageManager": "npm@11.6.3+sha512.4085a763162e0e3acd19a4e9d23ad3aa0978e501ccf947dd7233c12a689ae0bb0190763c4ef12366990056b34eec438903ffed38fde4fbd722a17c2a7407ee92",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Konstantin Karpman"
    }
  ],
  "scripts": {
    "start": "node --run start:openapi",
    "start:openapi": "redocly preview-docs openapi/openapi.json",
    "docs": "node --run docs:openapi",
    "docs:openapi": "redocly build-docs -o openapi/index.html openapi/openapi.json",
    "lint": "node --run lint:openapi",
    "lint:openapi": "redocly lint --format=github-actions openapi/openapi.json"
  },
  "devDependencies": {
    "@redocly/cli": "^2.14.7",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "semantic-release": "^25.0.2",
    "semantic-release-replace-plugin": "^1.2.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konsumation/openapi.git"
  },
  "bugs": {
    "url": "https://github.com/konsumation/openapi/issues"
  },
  "homepage": "https://github.com/konsumation/openapi#readme",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-replace-plugin",
        {
          "replacements": [
            {
              "files": [
                "openapi/openapi.json"
              ],
              "from": "\"0.0.0-semantic-release\"",
              "to": "\"${nextRelease.version}\"",
              "results": [
                {
                  "file": "openapi/openapi.json",
                  "hasChanged": true,
                  "numMatches": 1,
                  "numReplacements": 1
                }
              ],
              "countMatches": true
            }
          ]
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-npm",
      "arlac77/template-openapi",
      "konsumation/template"
    ]
  }
}
